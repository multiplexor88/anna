<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox fx:id="parent" stylesheets="@../../../../resources/styles/mainStyle.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.anna.gui.controllers.EmailController">
   <children>
      <GridPane id="gridPane" gridLinesVisible="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="80.0" prefWidth="600.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="205.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="505.0" minWidth="10.0" prefWidth="395.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="%key.mail.address">
               <opaqueInsets>
                  <Insets left="10.0" />
               </opaqueInsets>
            </Label>
            <Label text="%key.unspecified" GridPane.rowIndex="1">
               <opaqueInsets>
                  <Insets left="10.0" />
               </opaqueInsets>
            </Label>
            <TextField fx:id="toAddressTxt" prefHeight="50.0" prefWidth="500.0" GridPane.columnIndex="1" />
            <TextField fx:id="subjectTxt" prefHeight="50.0" prefWidth="500.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <TextArea fx:id="message" prefHeight="200.0" prefWidth="200.0" />
      <HBox id="hbox" alignment="CENTER_RIGHT" prefHeight="9.0" prefWidth="600.0">
         <children>
            <Button fx:id="okBtn" mnemonicParsing="false" text="%key.general.ok" />
            <Button fx:id="cancelBtn" mnemonicParsing="false" text="%key.general.cancel" />
         </children>
      </HBox>
   </children>
</VBox>
