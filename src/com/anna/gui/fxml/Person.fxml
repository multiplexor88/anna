<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<GridPane id="gridPane" fx:id="parent" gridLinesVisible="true" prefHeight="400.0" prefWidth="500.0" stylesheets="@../../../../resources/styles/mainStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.anna.gui.controllers.PersonController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="270.0" minWidth="10.0" prefWidth="150.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="248.0" minWidth="0.0" prefWidth="246.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="31.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Button fx:id="addressBtn" mnemonicParsing="false" onAction="#onAddress" prefHeight="50.0" prefWidth="271.0" text="..." GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Button fx:id="okBtn" mnemonicParsing="false" onAction="#onOk" prefHeight="86.0" prefWidth="328.0" text="%key.general.ok" GridPane.rowIndex="8" />
      <Button mnemonicParsing="false" prefWidth="40.0" text="First Name" visible="false" />
      <Button fx:id="contactBtn" mnemonicParsing="false" onAction="#onContact" prefHeight="50.0" prefWidth="317.0" text="..." GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <Button mnemonicParsing="false" prefWidth="40.0" text="First Name" visible="false" />
      <Button mnemonicParsing="false" prefWidth="40.0" text="First Name" visible="false" />
      <Button fx:id="occupationBtn" mnemonicParsing="false" onAction="#onOccupation" prefHeight="50.0" prefWidth="310.0" text="..." GridPane.columnIndex="1" GridPane.rowIndex="7" />
      <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#onCancel" prefHeight="94.0" prefWidth="368.0" text="%key.general.cancel" GridPane.columnIndex="1" GridPane.rowIndex="8" />
      <Button mnemonicParsing="false" prefHeight="22.0" prefWidth="14.0" text="Button" visible="false" />
      <Button mnemonicParsing="false" prefWidth="40.0" text="First Name" visible="false" />
      <Button fx:id="hobbyBtn" mnemonicParsing="false" onAction="#onHobby" prefHeight="50.0" prefWidth="295.0" text="..." GridPane.columnIndex="1" GridPane.rowIndex="6" />
      <Button mnemonicParsing="false" prefHeight="22.0" prefWidth="28.0" text="Button" visible="false" />
      <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <children>
            <RadioButton fx:id="maleCheck" mnemonicParsing="false" selected="true" text="%key.person.male">
               <toggleGroup>
                  <ToggleGroup fx:id="gender" />
               </toggleGroup>
               <padding>
                  <Insets right="10.0" top="5.0" />
               </padding>
            </RadioButton>
            <RadioButton fx:id="femaleCheck" mnemonicParsing="false" text="%key.person.female" toggleGroup="$gender">
               <padding>
                  <Insets top="5.0" />
               </padding>
            </RadioButton>
         </children>
      </HBox>
      <Label text="%key.person.firstName">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label text="%key.person.lastName" GridPane.rowIndex="1">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label text="%key.person.patronymic" GridPane.rowIndex="2">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label text="%key.person.gender" GridPane.rowIndex="3">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label text="%key.person.address" GridPane.rowIndex="4">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label text="%key.contact.title" GridPane.rowIndex="5">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label text="%key.event.hobbies" GridPane.rowIndex="6">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Label text="%key.event.occupations" GridPane.rowIndex="7">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <TextField fx:id="firstNameTxt" prefHeight="50.0" prefWidth="120.0" GridPane.columnIndex="1" />
      <TextField fx:id="lastNameTxt" prefHeight="50.0" prefWidth="120.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="patronymicTxt" prefHeight="50.0" prefWidth="120.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</GridPane>
